    // 编辑
    update(formName) {
      this.$refs[formName].validate(valid => {
        if (valid) {
          this.loading = true
          this.form.informationDOS = this.form.tableData
          analystUpdate(this.newForm)
            .then(res => {
              if (res.code === 200) {
                this.$message({
                  message: '保存分析任务成功',
                  showClose: true,
                  duration: 6000,
                  type: 'success'
                })
                this.$router.push({ path: '/sendTaskInfo/searchTaskInfo' })
                this.loading = false
              } else {
                this.loading = false
              }
            })
            .catch(() => {
              this.loading = false
            })
        } else {
          return false
        }
      })
    }
  }
}
