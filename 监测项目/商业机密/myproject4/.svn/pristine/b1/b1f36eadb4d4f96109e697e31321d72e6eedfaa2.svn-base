import request from '@/utils/request'

export function select(params) {
  return request({
    url: 'monitor/match/match-select',
    method: 'get',
    params
  })
}

export function add(data) {
  return request({
    url: 'monitor/match/add-match-information',
    method: 'post',
    data
  })
}

export function remove(params) {
  return request({
    url: 'monitor/match/match-delete',
    method: 'patch',
    params
  })
}

export function start(params) {
  return request({
    url: 'monitor/match/match-start',
    method: 'patch',
    params
  })
}

export function stop(params) {
  return request({
    url: 'monitor/match/match-stop',
    method: 'patch',
    params
  })
}

export function selectParam(params) {
  return request({
    url: 'monitor/match/select-param',
    method: 'get',
    params
  })
}

export function customAttribute(data) {
  return request({
    url: 'monitor/match/custom-attribute',
    method: 'post',
    data
  })
}

export function selectManager(params) {
  return request({
    url: 'monitor/match/select-manager',
    method: 'get',
    params
  })
}

export function deleteParam(params) {
  return request({
    url: 'monitor/match/delete-param',
    method: 'delete',
    params
  })
}
