          <el-col :span="24">
              <!-- <preliminary-judgment :lableWidth="114" ref="judgment"></preliminary-judgment> -->
                <el-form-item label="涉罪类型：" prop="preliminaryJudgment" :rules="[{ required: true, message: '请选择涉罪类型', trigger: 'blur' }]">
                      <el-select
