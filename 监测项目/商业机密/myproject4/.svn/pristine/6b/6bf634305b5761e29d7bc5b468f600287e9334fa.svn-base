                    <div style="margin-bottom:10px" v-for="(item,index) in scope.row.charosterAccountDOS" :key="index">
                      {{item.accountNum}}
                      <br/>
                    </div>
                  </template>
                  <span v-for="(item,index) in scope.row.charosterAccountDOS" :key="index">
                    <span>{{item.accountNum}}</span>
                  </span>
                </el-tooltip>
              </template>
