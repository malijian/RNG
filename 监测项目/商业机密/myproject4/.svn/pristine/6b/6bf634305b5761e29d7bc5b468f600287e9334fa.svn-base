<script>
import { centerRoster } from '@/api/sys-monitoringAnalysis/roster-warning/warning-task.js'
import { dictionary } from '@/api/sys-monitoringAnalysis/roster-warning/common.js'
import { commonPattern } from '@/utils/formValidate'
export default {
  data() {
    return {
      getRowKey(row) {
        return row.lcnId
      },
      optionsOne: [],
      dialogForm: {
        preliminaryJudgment: ''
      },
      options2: [],
      options3: [],
      loading: false,
      isReplenishOne: false,
      isReplenishTwo: false,
      preliminaryJudgment: '',
      tableData: [],
      selectArr: [],
      supplementOne: '',
      supplementTwo: '',
      dialogJudgmentData: [],
      params: {},
      pageInfo: {
        pageNum: 1,
        pageSize: 10,
        total: 0
      }
    }
  },
  computed: {
    searchParams() {
      const obj = {}
      var preliminaryJudgment = this.dialogForm.preliminaryJudgment

      if (!this.dialogForm.preliminaryJudgment) {
        preliminaryJudgment = ''
      }
      if (this.dialogForm.preliminaryJudgment.indexOf('1401') !== -1) {
        // const i = this.dialogForm.preliminaryJudgment.indexOf('1401')
        preliminaryJudgment = '1401-' + this.supplementOne
      }
      if (this.dialogForm.preliminaryJudgment.indexOf('1402') !== -1) {
        // const n = this.dialogForm.preliminaryJudgment.indexOf('1402')
        preliminaryJudgment = '1402-' + this.supplementTwo
      }
      obj.preliminaryJudgment = preliminaryJudgment
      return obj
    }
  },
  mounted() {
    this.getDictionary('TOSC')
    // this.fetchData()
  },
  methods: {
    // 获取数据字典
    getDictionary(params) {
      dictionary(params).then(res => {
        if (res.code === 200) {
          switch (params) {
            case 'TOSC':
              this.dialogJudgmentData = res.data
              break
            default:
              break
          }
        }
      })
    },
    onSubmit() {
      console.log('submit!')
    },
    // 切换分页条数
    handleSizeChange(size) {
      this.pageInfo.pageSize = size
      this.fetchData()
    },
    // 点击切换分页
    handleCurrentChange(pageNum) {
      this.pageInfo.pageNum = pageNum
      this.fetchData()
    },
    queryData() {
      this.pageInfo.pageNum = 1
      this.params = Object.assign({}, this.searchParams, this.pageInfo)
      this.fetchData()
    },
    // 请求数据
    fetchData() {
      this.$refs.searchForm.validate(valid => {
        if (valid) {
          this.loading = true
          this.params = Object.assign({}, this.params, this.pageInfo)
          centerRoster(this.params).then(res => {
            if (res.code === 200) {
              this.tableData = res.data.pageInfo.list
              this.tableData.forEach(res => {
                res.iNum = res.subjectINum
              })
              this.selectArr = this.tableData
              this.preliminaryJudgment = this.dialogForm.preliminaryJudgment
              this.pageInfo.total = res.data.pageInfo.total
              this.loading = false
            } else {
              this.loading = false
            }
          })
            .catch(err => {
              this.loading = false
              console.log(err)
            })
        }
      })
    },
    // 勾选table
    // handelSelect(rows) {
    //   this.selectArr = rows
    // },
    add() {
      if (this.selectArr.length > 0) {
        this.dataFn()
      } else {
        this.dialogFormVisible = false
        this.$emit('listFn', {
          Visible: this.dialogFormVisible
        })
      }
    },
    clearSelect() {
      this.dialogFormVisible = false
      this.$emit('listFn', {
        Visible: this.dialogFormVisible
      })
    },
    dataFn() {
      this.tableData = []
      this.dialogForm = {
        preliminaryJudgment: ''
      }
      const total = this.pageInfo.total
      this.pageInfo = {
        pageNum: 1,
        pageSize: 10,
        total: 0
      }
      this.$emit('listFn', {
        Visible: this.dialogFormVisible,
        selectArr: this.selectArr,
        total: total,
        preliminaryJudgment: this.preliminaryJudgment,
        showTableItem: 'center',
        tableData: this.tableData
      })
    },
    // 判断涉罪类型是否显示补充
    replenish() {
      if (this.dialogForm.preliminaryJudgment.indexOf('1402') !== -1) {
        this.isReplenishTwo = true
      } else {
        this.isReplenishTwo = false
      }
      if (this.dialogForm.preliminaryJudgment.indexOf('1401') !== -1) {
        this.isReplenishOne = true
      } else {
        this.isReplenishOne = false
      }
    },
    isValidInput(rule, value, callback) {
      if (!commonPattern.spaceBar.test(value)) {
        callback(new Error('内容不能含有空格'))
      } else if (commonPattern.specialChar.test(value) || commonPattern.specialEng.test(value)) {
        callback(new Error('内容不能填写特殊字符'))
      } else {
        callback()
      }
    },
    // 涉罪校验
    szisValidInput1(rule, value, callback) {
      if (!commonPattern.spaceBar.test(this.supplementOne)) {
        callback(new Error('内容不能含有空格'))
      } else if (
        commonPattern.specialChar.test(this.supplementOne) ||
        commonPattern.specialEng.test(this.supplementOne)
      ) {
        callback(new Error('内容不能填写特殊字符'))
      } else {
        callback()
      }
    },
    szisValidInput2(rule, value, callback) {
      if (!commonPattern.spaceBar.test(this.supplementTwo)) {
        callback(new Error('内容不能含有空格'))
      } else if (
        commonPattern.specialChar.test(this.supplementTwo) ||
        commonPattern.specialEng.test(this.supplementTwo)
      ) {
        callback(new Error('内容不能填写特殊字符'))
      } else {
        callback()
      }
    },
    cleanUp() {
      this.$refs.searchForm.resetFields()
      this.supplementTwo = ''
      this.supplementOne = ''
      this.isReplenishOne = false
      this.isReplenishTwo = false
    }
  }
}
</script>

