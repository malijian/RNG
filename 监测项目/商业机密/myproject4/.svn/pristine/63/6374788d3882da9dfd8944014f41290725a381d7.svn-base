import request from '@/utils/request'

export function getList(params) {
  /**
   * 通知公告
   */
  return request({
    url: '/dashboard/list',
    method: 'get',
    params
  })
}

export function getDetail(params) {
  /**
   * 详情
   */
  return request({
    url: '/detail',
    method: 'get',
    params: { id: params }
  })
}
